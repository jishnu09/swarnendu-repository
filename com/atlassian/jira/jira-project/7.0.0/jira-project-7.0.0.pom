        <profile>
            <id>deploy-source-jars-snapshots</id>
            <!--
                This profile is here to deploy sources directly using maven deploy plugin
                since deploying SNAPSHOTs using maven-source-routes-plugin does not work
                with Maven 3 -->
            <build>
                <plugins>
                    <plugin>
                        <artifactId>maven-source-plugin</artifactId>
                        <version>${maven.source.plugin.version}</version>
                        <executions>
                            <execution>
                                <id>attach-sources</id>
                                <goals>
                                    <goal>jar</goal>
                                </goals>
                                <configuration>
                                    <attach>true</attach>
                                </configuration>
                            </execution>
                        </executions>
                    </plugin>
                    <plugin>
                        <groupId>org.apache.maven.plugins</groupId>
                        <artifactId>maven-enforcer-plugin</artifactId>
                        <version>1.3.1</version>
                        <executions>
                            <execution>
                                <id>allow-only-for-snapshot</id>
                                <goals>
                                    <goal>enforce</goal>
                                </goals>
                                <phase>validate</phase>
                                <configuration>
                                    <rules>
                                        <evaluateBeanshell>
                                            <condition>
                                                "${project.version}".endsWith("SNAPSHOT")
                                            </condition>
                                            <message>Profile deploy-source-jars-snapshots should only be used when deploying SNAPSHOT. Otherwise use deploy-source-jars so that source jars are correctly deployed to private repository.</message>
                                        </evaluateBeanshell>
                                    </rules>
                                    <skip>${skip.deploy.source.jars.snapshots.only.enforcer}</skip>
                                </configuration>
                            </execution>
                        </executions>
                    </plugin>
                </plugins>
            </build>
